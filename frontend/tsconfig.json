{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "jsx": "react",
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true,  
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "emitDecoratorMetadata": false,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }
  